			"properties": {
				"type": "UDFLibrary",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [],
					"scriptLines": [
						"gendertointeger(string) as integer = iif(i1 == 'male',1,iif(i1 == 'female',0,-1))"
					]
				}
			},
			"dependsOn": []
		},
